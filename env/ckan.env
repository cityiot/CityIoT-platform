CKAN_OAUTH2_PROFILE_API_FULLNAME_FIELD=
CKAN_OAUTH2_PROFILE_API_MAIL_FIELD=
CKAN_OAUTH2_AUTHORIZATION_HEADER=
CKAN_OAUTH2_LEGACY_IDM=

CKAN_PRIVATEDATASETS_SHOW_ACQUIRE_URL_ON_CREATE=
CKAN_PRIVATEDATASETS_SHOW_ACQUIRE_URL_ON_EDIT=
