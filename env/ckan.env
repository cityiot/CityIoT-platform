# See https://github.com/conwetlab/FIWARE-CKAN-Extensions/blob/master/doc/installation-administration-guide.rst
# and https://hub.docker.com/r/fiware/ckan-extended
# for explanations for the parameters

# When running locally, CKAN_SITE_URL must contain the port
CKAN_SITE_URL=https://ckan.tlt-cityiot.rd.tuni.fi
CKAN_MAX_UPLOAD_SIZE_MB=2048

# Basic Configuration parameters
# Defaults work with linked containers, change to use own Postgres, SolR, Redis or Datapusher
CKAN_SOLR_URL=http://ckan_solr:8983/solr/ckan
CKAN_REDIS_URL=redis://redis:6379/1
CKAN_DATAPUSHER_URL=http://ckan_datapusher:8800
CKAN_VERIFY_REQUESTS=True

POSTGRES_SERVICE=postgresdb

# DON'T USE THIS SETTING ON PRODUCTION!
OAUTHLIB_INSECURE_TRANSPORT=True

CKAN_OAUTH2_REGISTER_URL=
